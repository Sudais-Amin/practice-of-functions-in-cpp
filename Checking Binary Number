#include<iostream>
#include<string>
using namespace std;

int check(string s){
	int chk = 0;
	for(int i=0 ; i<s.length() ; i++)
	{
		if(s.at(i) != '1' && s.at(i) != '0')
	{
		cout<<endl<<endl<<"NOT Binary...!";
		return 0;}	
	}
	cout<<endl<<endl<<s<<" is Binary Number.";
	return 1;
}
int main(){
	string s ;
	cout<<"Enter the Binary Number : "; cin>>s;
	check(s);
}
